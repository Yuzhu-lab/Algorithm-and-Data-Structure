import numpy as np
class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        return np.transpose(np.reshape(nums, (len(nums)//n, n))).flatten()

class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        res = []
        for x, y in zip(nums[:n], nums[n:]):
            res.append(x)
            res.append(y)
        return res
        
