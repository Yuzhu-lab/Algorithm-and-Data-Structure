class Solution:
    def mergeTwoLists(self, l1: ListNode, l2: ListNode) -> ListNode: 
        # A listnode to append answer
        dummy = ListNode(-1)
        # A ListNode that point to very start of dummy
        ans = dummy
        while l1 and l2:
            if l1.val > l2.val:
                dummy.next = l2
                l2 = l2.next
            else:
                dummy.next = l1
                l1 = l1.next
            dummy = dummy.next
        if l1:
            dummy.next = l1
        else:
            dummy.next = l2
        return ans.next
